// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Cache {
  id         Int      @id @default(autoincrement())
  type       String
  parameter  Int
  is_expired Boolean
  expires_at DateTime
  value      String
}
